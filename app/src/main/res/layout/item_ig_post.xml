<?xml version="1.0" encoding="utf-8"?>
<com.dolphpire.instamanage.views.DolphPireRL xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@color/colorSelected"
    android:layout_margin="2dp">

    <com.dolphpire.instamanage.views.DolphPireIS
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:src="#47AFA6"/>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:layout_alignParentBottom="true"
        android:background="#4DFFFFFF">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:orientation="horizontal"
            android:layout_centerHorizontal="true"
            android:background="#A6FFFFFF">

            <TextView
                android:id="@+id/title_toolbar"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:clickable="false"
                android:focusable="false"
                android:fontFamily="@font/open_sans_semibold"
                android:text="3645"
                android:textColor="#000000"
                android:textSize="16sp"
                tools:ignore="MissingPrefix" />

            <ImageView
                android:id="@+id/imgLikes"
                android:layout_width="17dp"
                android:layout_height="17dp"
                android:src="@drawable/ic_ig_like"/>

        </LinearLayout>

    </RelativeLayout>

</com.dolphpire.instamanage.views.DolphPireRL>